{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "ES6",
    "module": "commonjs",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
  },
  "include": ["./**/*"]
}
